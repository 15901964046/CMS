{
    "name": "graham-campbell/bootstrap-cms",
    "description": "Bootstrap CMS Is A PHP CMS Powered By Laravel 4.0 With Sentry 2.1",
    "keywords": ["laravel", "framework", "sentry", "bootstrap", "cms", "bootstrap cms", "Bootstrap CMS", "Bootstrap-CMS", "blog", "login", "Graham Campbell", "GrahamCampbell"],
    "license": "GNU Affero General Public License v3",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "graham-campbell/cms-core": "0.1.*",
        "fideloper/proxy": "2.0.*",
        "davejamesmiller/laravel-boris": "1.0.*",
        "barryvdh/laravel-debugbar": "0.3.*"
    },
    "require-dev": {
        "mockery/mockery": "0.8.*",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "classmap": [
            "app/database/migrations",
            "app/database/seeds"
        ],
        "psr-0": {
            "GrahamCampbell\\BootstrapCMS": "app/src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
