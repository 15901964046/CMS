{
    "name": "graham-campbell/bootstrap-cms",
    "description": "Bootstrap CMS Is A PHP CMS Powered By Laravel 4.1 With CMS Core And Sentry 2",
    "keywords": ["laravel", "framework", "sentry", "bootstrap", "cms", "bootstrap cms", "Bootstrap CMS", "Bootstrap-CMS", "blog", "login", "Graham Campbell", "GrahamCampbell"],
    "license": "AGPL-3.0",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/dropbox-sdk-php"
        },
        {
            "type": "vcs",
            "url": "https://github.com/GrahamCampbell/asset"
        },
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.1.*",
        "symfony/config": "2.4.*",
        "symfony/filesystem": "2.4.*",
        "graham-campbell/cms-core": "0.2.*",
        "graham-campbell/cms-logviewer": "0.2.*",
        "fideloper/proxy": "2.0.*",
        "barryvdh/laravel-debugbar": "1.2.*"
    },
    "require-dev": {
        "graham-campbell/testbench": "0.2.*"
    },
    "autoload": {
        "classmap": [
            "app/database/migrations",
            "app/database/seeds"
        ],
        "psr-0": {
            "GrahamCampbell\\BootstrapCMS": "src/",
            "GrahamCampbell\\Tests\\BootstrapCMS": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "0.6-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
