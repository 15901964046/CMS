{
    "name": "graham-campbell/bootstrap-cms",
    "description": "Bootstrap CMS Is A PHP CMS Powered By Laravel 4.0 With Sentry 2.1",
    "keywords": ["laravel", "framework", "sentry", "bootstrap", "cms", "bootstrap cms", "Bootstrap CMS", "Bootstrap-CMS", "blog", "login", "Graham Campbell", "GrahamCampbell"],
    "license": "GNU Affero General Public License v3",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@mineuk.com"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "sebastianbergmann/phpcov",
                "version": "1.1.0",
                "dist": {
                    "url": "https://github.com/sebastianbergmann/phpcov/archive/1.1.0.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/sebastianbergmann/phpcov.git",
                    "type": "git",
                    "reference": "1.1.0"
                },
                "bin": [
                    "phpcov.php"
                ]
            }
        }
    ],
    "require": {
        "php": ">=5.4.7",
        "laravel/framework": "4.0.*",
        "symfony/config": "2.3.*",
        "symfony/filesystem": "2.3.*",
        "graham-campbell/cms-core": "dev-develop",
        "fideloper/proxy": "2.0.*",
        "davejamesmiller/laravel-boris": "dev-master#c08521f99f36146b8ec9430bf5985c56444c0e10",
        "barryvdh/laravel-debugbar": "0.3.*"
    },
    "require-dev": {
        "orchestra/testbench": "2.0.*",
        "mockery/mockery": "0.8.*",
        "phpunit/phpunit": "3.7.*",
        "sebastian/finder-facade": "1.1.*",
        "sebastianbergmann/phpcov": "1.1.0",
        "satooshi/php-coveralls": "dev-master",
        "symfony/event-dispatcher": "2.3.*",
        "symfony/stopwatch": "2.3.*",
        "symfony/yaml": "2.3.*",
        "guzzle/guzzle": "3.8.*"
    },
    "autoload": {
        "classmap": [
            "app/database/migrations",
            "app/database/seeds"
        ],
        "psr-0": {
            "GrahamCampbell\\BootstrapCMS": "src/",
            "GrahamCampbell\\Tests\\BootstrapCMS": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "pre-update-cmd": [
            "php artisan clear-compiled"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
